    "version": "0.2.0",
    "configurations": [
        {
            "name": "Book Fischer",
            "type": "node",
            "request": "launch",
            "args": [
                "book",
                "Fischer",
                "sofort",
                "hallo@foo.bar",
                "2",
                "-v"
            ],
            "runtimeArgs": [
                "-r",
                "${workspaceRoot}/.pnp.cjs",
                "-r",
                "ts-node/register",
                "${workspaceRoot}/src/main.ts"
            ],
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "help",
            "type": "node",
            "request": "launch",
            "args": [
                "help"
            ],
            "runtimeArgs": [
                "-r",
                "${workspaceRoot}/.pnp.cjs",
                "-r",
                "ts-node/register",
                "${workspaceRoot}/src/main.ts"
            ],
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}